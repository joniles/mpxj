<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>net.sf.mpxj-for-vb</id>
        <title>MPXJ for VB</title>
        <version>0.0.0</version>
        <authors>Jon Iles</authors>
        <licenseUrl>https://sourceforge.net/p/mpxj/mpxj/ci/master/tree/legal/licence.txt</licenseUrl>
        <projectUrl>http://mpxj.sf.net</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>MPXJ is an open source file handling library for Java and .Net to read and write Microsoft Project MPX and MSPDI XML files, Planner files, Primavera XER files and databases, and MPP, MPT, and MPD files for Project 98, 2000, 2002, 2003, 2007, 2010, 2013 and 2016. This version provides properties, rather than Java style getters, setters.</description>
        <summary>MPXJ reads and writes schedule data in a variety of formats.</summary>
    </metadata>
    <files>
    	<file src="..\lib.net\mpxj-for-vb.*" target="lib"/>
    	<file src="..\lib.net\IKVM.*" target="lib"/>
    	<file src="..\lib.net\poi*.*" target="lib"/>
    	<file src="..\lib.net\rtfparserkit*.*" target="lib"/> 
    	<file src="..\lib.net\sqlite-jdbc*.*" target="lib"/>
    	<file src="..\lib.net\MpxjUtilities.*" target="lib"/>
    	<file src="..\lib.net\ikvm-native-win32-x86.dll" target="build"/>
    	<file src="..\lib.net\ikvm-native-win32-x64.dll" target="build"/>
    	<file src="net.sf.mpxj-for-vb.targets" target="build"/>    	
    </files>
</package>