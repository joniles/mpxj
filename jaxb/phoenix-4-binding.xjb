<jxb:bindings version="3.0"
               xmlns:jxb="https://jakarta.ee/xml/ns/jaxb"
               xmlns:xs="http://www.w3.org/2001/XMLSchema"
               schemaLocation="phoenix-4.xsd">

   <jxb:globalBindings>
		<jxb:javaType name="java.lang.Integer"
                    xmlType="xs:int"
                    parseMethod="org.mpxj.phoenix.DatatypeConverter.parseInteger"
                    printMethod="org.mpxj.phoenix.DatatypeConverter.printInteger"/>

		<jxb:javaType name="java.time.LocalDateTime"
                    xmlType="xs:dateTime"
                    parseMethod="org.mpxj.phoenix.DatatypeConverter.parseDateTime"
                    printMethod="org.mpxj.phoenix.DatatypeConverter.printDateTime"/>

		<jxb:javaType name="org.mpxj.Duration"
                    xmlType="phoenixDuration"
                    parseMethod="org.mpxj.phoenix.DatatypeConverter.parseDuration"
                    printMethod="org.mpxj.phoenix.DatatypeConverter.printDuration"/>

		<jxb:javaType name="java.util.UUID"
                    xmlType="phoenixUuid"
                    parseMethod="org.mpxj.phoenix.DatatypeConverter.parseUUID"
                    printMethod="org.mpxj.phoenix.DatatypeConverter.printUUID"/>

		<jxb:javaType name="org.mpxj.RelationType"
                    xmlType="phoenixRelationType"
                    parseMethod="org.mpxj.phoenix.DatatypeConverter.parseRelationType"
                    printMethod="org.mpxj.phoenix.DatatypeConverter.printRelationType"/>

		<jxb:javaType name="org.mpxj.TimeUnit"
                    xmlType="phoenixTimeUnits"
                    parseMethod="org.mpxj.phoenix.DatatypeConverter.parseTimeUnits"
                    printMethod="org.mpxj.phoenix.DatatypeConverter.printTimeUnits"/>

		<jxb:javaType name="org.mpxj.ResourceType"
                    xmlType="phoenixResourceType"
                    parseMethod="org.mpxj.phoenix.DatatypeConverter.parseResourceType"
                    printMethod="org.mpxj.phoenix.DatatypeConverter.printResourceType"/>

        <jxb:javaType name="java.time.DayOfWeek"
                    xmlType="phoenixDay"
                    parseMethod="org.mpxj.phoenix.DatatypeConverter.parseDay"
                    printMethod="org.mpxj.phoenix.DatatypeConverter.printDay"/>

		<jxb:javaType name="java.time.LocalDateTime"
                    xmlType="phoenixFinish"
                    parseMethod="org.mpxj.phoenix.DatatypeConverter.parseFinishDateTime"
                    printMethod="org.mpxj.phoenix.DatatypeConverter.printFinishDateTime"/>

   </jxb:globalBindings>

</jxb:bindings>
